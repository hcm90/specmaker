<div class="small-11 medium-9 large-9 large-centered columns">
<h4>Log in</h4>
<div class = "panel">
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %><br/>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Log in", :class => "button expand" %>
<% end %>
</div>
</div>